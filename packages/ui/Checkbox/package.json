{
  "name": "@kit-base/checkbox",
  "version": "0.3.6",
  "description": "Чекбокс для проекта",
  "license": "UNLICENSED",
  "sideEffects": [
    "*.css"
  ],
  "publishConfig": {
    "access": "public",
    "main": "build/index.js",
    "types": "build/index.d.ts",
    "type": "git",
    "registry": "https://gitlab.com/api/v4/projects/50873891/packages/npm/"
  },
  "main": "src/index",
  "types": "src/index.ts",
  "scripts": {
    "build": "run -T rimraf build && yarn run -T rollup -c --bundleConfigAsCjs && yarn setVersion ",
    "build:raw": "run -T tsc -p tsconfig.build.json",
    "build:watch": "yarn build:raw --watch",
    "pub": "yarn build && yarn setPath && npm publish && yarn revertPath",
    "setPath": "node ../../../fe-scripts/bin/set-paths.js",
    "revertPath": "node ../../../fe-scripts/bin/revert-paths.js",
    "test": "run -T pcbl-fe  test",
    "checkDep": "run -T pcbl-fe  checkDependencies",
    "pack": "run -T pcbl-fe  pack",
    "revertPack": "run -T pcbl-fe  revertPack",
    "setVersion": "run -T pcbl-fe  setVersion",
    "clean": "run -T pcbl-fe  clean"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.pcbltools.ru/bitbucket/projects/EDUPOWER/repos/uikit4/browse/packages/ui/Checkbox",
    "directory": "packages/ui"
  },
  "files": [
    "/build"
  ],
  "dependencies": {
    "classnames": "2.3.1"
  },
  "devDependencies": {
    "@kit-base/common-types": "0.0.2"
  },
  "peerDependencies": {
    "react": "*"
  }
}
